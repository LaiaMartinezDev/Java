package Alternativas;

import java.util.Scanner;

public class Ejercicio03IfAnidados {
	/*  Vamos a comparar dos numeros obtenidos por consola */
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scanner = new Scanner (System.in);
		int numero1 = 0, numero2=0;
		System.out.println("Dame un numero entero");
		numero1 = scanner.nextInt();
		System.out.println("Dame otro numero entero");
		numero2 = scanner.nextInt();
		
		if (numero1 > numero2) {
			System.out.println("El primer numero es mayor que el segundo numero.");
		} else {
			if(numero1 < numero2) {
			System.out.println("El primer numero es menor que el segundo numero.");
		} else {
			System.out.println("El primer numero es igual que el segundo numero.");
				}
			}

		System.out.println("PROCESO TERMINADO");
	}
}
