package Ejecutables;

public class Ejercicio04MasMasMenosMenos {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int contador= 0;
		
		contador++;
		System.out.println(contador);
		contador--;
		System.out.println(contador);
		++contador;
		System.out.println(contador);
		--contador;

	}

}
