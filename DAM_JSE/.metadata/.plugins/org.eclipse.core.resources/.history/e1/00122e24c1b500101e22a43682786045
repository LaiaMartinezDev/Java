package Ejecutables;

public class Ejercicio04MasMasMenosMenos {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int contador= 0;
		
		contador++;
		System.out.println(contador);
		contador--;
		System.out.println(contador);
		++contador;
		System.out.println(contador);
		--contador;
		System.out.println(contador);
		System.out.println("MAS MAS EN SYSO AFTER");
		
		contador = 7;
		System.out.println(contador++); //Here its going to print 7 bc contador goes before the ++
		System.out.println(contador); // Here its going to print 8 bc contador goes after the ++
		
		contador = 7;
		System.out.println(++contador); //Here its going to print 7 bc contador goes before the ++
		System.out.println(contador); // Here its going to print 6 bc contador goes after the ++

	}

}
