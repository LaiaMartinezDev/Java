package Ejecutables;

public class Ejercicio04MasMasMenosMenos {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int contador= 0;
		
		contador++;
		System.out.println(contador);
		contador--;
		System.out.println(contador);
		++contador;
		System.out.println(contador);
		--contador;
		System.out.println(contador);
		System.out.println("MAS MAS EN SYSO");
		
		contador = 7;
		System.out.println(contador++);
		System.out.println(contador);

	}

}
